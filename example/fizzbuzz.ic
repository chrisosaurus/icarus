
fn is_div(a::Sint, b::Sint) -> Bool
    return (a % b) == 0
end

fn fizzbuzz(from::Sint, to::Sint)
    for num in range(from, to)
        let &arr::Vector[String]

        if is_div(num, 3)
            &arr.append("Fizz")
        end

        if is_div(num, 5)
            &arr.append("Buzz")
        end

        if length(arr) == 0
            &arr.append(num)
        end

        print(arr.join(" "))
    end
end

fn main()
    fizzbuzz(1, 100)
end

