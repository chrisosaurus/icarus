
fn is_div(a::Int, b::Int) -> Bool
    return not(a % b)
end

fn fizzbuzz(from::Int, to::Int)
    for num in range(from to)
        let &arr = []

        if is_div(num, 3)
            &arr.append("Fizz")
        end

        if is_div(num, 5)
            &arr.append("Buzz")
        end

        if length(arr) == 0
            &arr.append(num)
        end

        print(join(" ", arr))
    end
end

fn main()
    fizzbuzz(1, 100)
end

