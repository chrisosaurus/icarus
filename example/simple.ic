# user defined type with 2 fields, a Sint and a String
type Foo
    a::Sint
    b::String
end

# simple function to test return values and branching
fn maybe_add_one(b::Bool, i::Sint) -> Sint
    if b
        return i+1s
    else
        return i
    end
end

# entry point for program
fn main()
    let f::Foo = Foo(maybe_add_one(True, 1s), "hello")
    println(f)

    let x = 6s
    if x > 14s
        println("x > 14")
    else
        println("x <= 14")
    end
end

