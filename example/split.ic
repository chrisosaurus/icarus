builtin fn index_of(str::String, c::Char) -> Maybe[Sint]

fn split(str::String, c::Char) -> [String]
    let idx = index_of(s, c)
    match idx
        case something::Sint
            let before = s[0s,   something]
            let after  = s[something, len(s)]
            return [before, after]
        else
            return [str]
    end
end


